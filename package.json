{
  "name": "unconfigured-eslint-rules",
  "version": "1.0.1",
  "description": "Detect unconfigured ESLint rules",
  "repository": "shinnn/unconfigured-eslint-rules",
  "scripts": {
    "pretest": "eslint --fix --config @shinnn/node index.js test/test.js",
    "test": "node --strong_mode test/test.js",
    "coverage": "node --strong_mode node_modules/.bin/istanbul cover test/test.js"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "keywords": [
    "eslint",
    "setting",
    "config",
    "configuration",
    "array",
    "list",
    "rule",
    "rules",
    "check",
    "detect",
    "unconfigured"
  ],
  "dependencies": {
    "all-eslint-rules": "^1.0.1",
    "array-difference": "^0.0.1",
    "configured-eslint-rules": "^1.0.0"
  },
  "devDependencies": {
    "@shinnn/eslint-config-node": "^1.0.1",
    "eslint": "^1.10.3",
    "istanbul": "^0.4.1",
    "tape": "^4.4.0"
  }
}
